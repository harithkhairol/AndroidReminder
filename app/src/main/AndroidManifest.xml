<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.harithproperties.remindernew">

    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="New Reminder"
        android:theme="@style/AppTheme" >
        <activity
            android:name="net.harithproperties.remindernew.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="net.harithproperties.remindernew.NextActivity"
            android:label="Next Screen">
        </activity>

        <receiver android:name=".MyReceiver"/>


    </application>

</manifest>